{
  "name": "buoy-api",
  "version": "0.0.1",
  "description": "Proxy API for processing and serving ERDDAP data",
  "main": "app/server.js",
  "author": "Fernando Gelin, Mary McGrath",
  "license": "MIT",
  "engines": {
    "node": ">16.0.0"
  },
  "scripts": {
    "start": "node app/server.js",
    "dev": "nodemon app/server.js",
    "dev:local": "BUOY_API_ERDDAP_URL=http://localhost:8080/erddap/tabledap nodemon app/server.js",

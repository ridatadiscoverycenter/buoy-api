    "prettier": "^2.3.1"
  },
  "dependencies": {
    "arquero": "^4.8.1",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "lodash": "^4.17.20",
    "memory-cache": "^0.2.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.0.1",
